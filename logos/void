BG=$(printf "\033[48;2;34;34;34m") # Gray (bg)
ODFC1=$(printf '\033[38;2;171;194;171m') # Light Green
ODFC2=$(printf '\033[38;2;71;128;97m') # Dark Green
C1="$ODFC1"; C2="$ODFC2"
C3=$(printf '\033[38;2;79;87;79m') # Gray
ODFLOGO=$(cat <<EOF
${BG}${C1} ____ 
${BG}${C2}/${C1}    \\
${BG}${C3}VO${C1}()${C3}ID
${BG}${C2}\____${C1}/
EOF
)
#  ____
# /    \
# VO()ID
# \____/

# BG
#1  ____
#2 /1    \
#3 VO1()3ID
#2 \____1/
